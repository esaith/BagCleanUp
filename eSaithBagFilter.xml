            <Button name="$parentTab5" inherits="CharacterFrameTabButtonTemplate" id="5" text="Epic">
                <Anchors>
                    <Anchor point="LEFT" relativePoint="RIGHT" relativeTo="$parentTab4" x="-15"/>
                </Anchors>
                <Scripts>
                    <OnShow>
                        PanelTemplates_TabResize(self, 0)
                        PanelTemplates_SetTab(eSaithBagFilterILVLBottomTabs, self:GetID());
                    </OnShow>
                    <OnClick>
                        eSaithBagFilterBottomTab_Click(self, "OnClick" )
                        PanelTemplates_SetTab(eSaithBagFilterILVLBottomTabs, self:GetID());
                    </OnClick>
                </Scripts>
            </Button>
        </Frames>
        <Scripts>
            <OnLoad function="eSaithBagFilterILVLBottomTabs_OnLoad" />
            <OnShow function="eSaithBagFilterILVLBottomTabs_OnShow" />
        </Scripts>
