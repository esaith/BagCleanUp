<Ui xmlns="http://www.blizzard.com/wow/ui/"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.blizzard.com/wow/ui/ 
                   ..\..\FrameXML\UI.xsd">
    <Script File="eSaithBagFilter.lua"/>
    <Frame name="eSaithBagFilterSliderTemplate" virtual="true">
        <Size x="300" y="50" />
        <Frames>
            <CheckButton name="$parentCheckButton" inherits="UICheckButtonTemplate">
                <Size x="30" y="30" />
                <Anchors>
                    <Anchor point="LEFT" relativeTo="$parent" relativePoint="LEFT" />
                </Anchors>
                <Scripts>
                    <OnClick function="eSaithBagFilterSlider_CheckBoxClick" />
                </Scripts>
            </CheckButton>
            <Button name="$parentDownButton">
                <Size x="20" y="20" />@
                <Anchors>
                    <Anchor point="LEFT" relativeTo="$parentCheckButton" relativePoint="RIGHT" x="1" />
                </Anchors>
                <Layers>
                    <Layer level="LAYOUT" setAllPoints="true">
                        <Texture name="$parentBackground" file="Interface\BUTTONS\UI-MinusButton-Up" />
                    </Layer>
                </Layers>
                <PushedTexture name="$parent_buttonPushed" file="Interface\BUTTONS\UI-MinusButton-Down" />
                <Scripts>
                    <OnClick function="eSaithBagFilterSlider_DownButton"/>
                </Scripts>
            </Button>
            <Slider name="$parentSlider" inherits="OptionsSliderTemplate"  minValue="0" maxValue="800" valueStep="1">
                <Size x="200"  y="20"/>
                <Anchors>
                    <Anchor point="LEFT" relativeTo="$parentDownButton" relativePoint="RIGHT" x="3"/>
                </Anchors>
                <Layers>
                    <Layer>
                        <FontString name="$parentTitle" inherits="GameFontNormal" text="">
                            <Anchors>
                                <Anchor point="TOP" relativeTo="$parent" relativePoint="BOTTOM" />
                            </Anchors>
                            <Color r=".7" g=".7" b="0" />
                        </FontString>
                        <FontString name="$parentValue" inherits="GameFontNormal" text="">
                            <Anchors>
                                <Anchor point="BOTTOM" relativeTo="$parent" relativePoint="TOP"/>
                            </Anchors>
                            <Color r=".7" g=".7" b="0" />
                        </FontString>
                    </Layer>
                </Layers>
                <Scripts>
                    <OnLoad function="eSaithBagFilterSlider_OnLoad" />
                    <OnValueChanged function="eSaithBagFilterSlider_SliderValueChanged" />
                </Scripts>
            </Slider>
            <Button name="$parentUpButton">
                <Size x="20" y="20" />
                <Anchors>
                    <Anchor point="LEFT" relativeTo="$parentSlider" relativePoint="RIGHT" x="5"/>
                </Anchors>
                <Layers>
                    <Layer level="LAYOUT" setAllPoints="true">
                        <Texture name="$parent_ButtonRest" file="Interface\BUTTONS\UI-PlusButton-Up" />
                    </Layer>
                </Layers>
                <PushedTexture name="$parent_ButtonPushed" file="Interface\BUTTONS\UI-PlusButton-Down" />
                <Scripts>
                    <OnClick function="eSaithBagFilterSlider_UpButton"/>
                </Scripts>
            </Button>
        </Frames>
    </Frame>

    <Frame name="eSaithBagFilter" parent="UIParent" hidden="true" frameStrata="HIGH" inherits="BasicFrameTemplateWithInset" movable="true">        
        <Anchors>
            <Anchor point="CENTER" relativeTo="UIParent" relativePoint="CENTER" />
        </Anchors>
        <Layers>
            <Layer level="BACKGROUND" setAllPoints="true">
                <Texture name="$parentBackground">
                    <Color r="1.0" g="1.0" b="1.0" a=".8" />o
                </Texture>
                <FontString name="$parentTitle" inherits="GameFontNormal" text="Bag Clean Up Options">
                    <Anchors>
                        <Anchor point="TOP" relativeTo="$parent" relativePoint="TOP" y="-5"/>
                    </Anchors>
                </FontString>
            </Layer>
        </Layers>
        <Scripts>
            <OnLoad function="eSaithBagFilter_OnLoad"/>
            <OnEvent function="eSaithBagFilter_OnEvent" />
            <OnDragStart>self:StartMoving();</OnDragStart>
            <OnDragStop function="eSaithBagFilter_OnStopDrag" />
            <OnShow function="eSaithBagFilter_OnShow" />
        </Scripts>
    </Frame>

    <Button name="eSaithBagFilterItemButtonTemplate" parent="eSaithBagFilter" virtual="true" inherits="UICheckButtonTemplate">
        <Size x="40" y="40"/>
    </Button>


    <Button name="$parentSellButton" parent="eSaithBagFilter" movable="true" enableMouse="true" hidden="true" frameStrata="HIGH" topLevel="true">
        <Size x="50" y="50"/>
        <Anchors>
            <Anchor point="TOP" relativePoint="TOPRIGHT" relativeTo="$parent" x="-35" y="-25"/>
        </Anchors>
        <Layers>
            <Layer level="HIGH">
                <Texture name="$parentTexture" file="Interface\BUTTONS\UI-GroupLoot-Coin-Up" >
                    <Size x="40" y="40"/>
                    <Anchors>
                        <Anchor point="CENTER" relativePoint="CENTER" />
                    </Anchors>
                </Texture>
            </Layer>
        </Layers>
        <PushedTexture name="$parent_buttonPushed" file="Interface\BUTTONS\UI-GroupLoot-Coin-Highlight" >
            <Size x="40" y="40"/>
            <Anchors>
                <Anchor point="CENTER" relativePoint="CENTER" />
            </Anchors>
        </PushedTexture>
        <Scripts>
            <OnLoad>self.TimeSinceLastUpdate = 0</OnLoad>
            <OnClick function="eSaithBagFilterSellButton_Click" />
            <OnUpdate function="eSaithBagFilterSellButton_OnUpdate" />
            <OnEnter function="eSaithBagFilterSellButton_OnEnter" />
            <OnLeave>GameTooltip:Hide()</OnLeave>
            <OnHide function="eSaithBagFilterSellButton_OnHide" />
        </Scripts>
    </Button>

    <Frame name="$parentSideTabs" parent="eSaithBagFilter">
        <Size x="60" y="180" />
        <Anchors>
            <Anchor point="TOPRIGHT" relativeTo="$parent" relativePoint="TOPLEFT" x="10" y="-1"/>
        </Anchors>
        <Frames>
            <Button name="$parentTab1" inherits="ItemButtonTemplate" id="1" >
                <Anchors>
                    <Anchor point="TOP" relativePoint="TOP" relativeTo="$parent" />
                </Anchors>
                <Layers>
                    <Layer level="OVERLAY" setAllPoints="true">
                        <Texture name="$parentTexture" file="Interface\BUTTONS\UI-MicroButton-World-Up" >
                            <Size x="40" y="60"/>
                            <Anchors>
                                <Anchor point="CENTER" relativePoint="CENTER" y="10"/>
                            </Anchors>
                        </Texture>
                    </Layer>
                </Layers>
                <Scripts>
                    <OnClick>
                        eSaithBagFilter_ShowFilterZone(self, button)
                        eSaithBagFilterSideTabs_OnClick(self, button)
                    </OnClick>
                    <OnShow>
                        CharacterFrame_TabBoundsCheck(self);
                    </OnShow>
                    <OnEnter function="eSaithBagFilterTab_OnEnter" />
                    <OnLeave function="OnGameToolTipLeave" />
                </Scripts>
            </Button>
            <Button name="$parentTab2" inherits="ItemButtonTemplate" id="2">
                <Anchors>
                    <Anchor point="TOP" relativePoint="BOTTOM" relativeTo="$parentTab1" />
                </Anchors>
                <Layers>
                    <Layer level="OVERLAY" setAllPoints="true">
                        <Texture name="$parentTexture" file="Interface\BUTTONS\UI-MicroButton-Spellbook-Up" >
                            <Size x="40" y="60"/>
                            <Anchors>
                                <Anchor point="CENTER" relativePoint="CENTER" y="10"/>
                            </Anchors>
                        </Texture>
                    </Layer>
                </Layers>
                <Scripts>
                    <OnClick>
                        eSaithBagFilter_ShowFilteriLVL(self, buton)
                        eSaithBagFilterSideTabs_OnClick(self, button)
                    </OnClick>
                    <OnShow>
                        CharacterFrame_TabBoundsCheck(self);
                    </OnShow>
                    <OnEnter function="eSaithBagFilterTab_OnEnter" />
                    <OnLeave function="OnGameToolTipLeave" />
                </Scripts>
            </Button>
            <Button name="$parentTab3" inherits="ItemButtonTemplate" id="3">
                <Anchors>
                    <Anchor point="TOP" relativePoint="BOTTOM" relativeTo="$parentTab2" />
                </Anchors>
                <Layers>
                    <Layer level="OVERLAY" setAllPoints="true">
                        <Texture name="$parentTexture" file="Interface\BUTTONS\UI-MicroButton-Raid-Up" >
                            <Size x="40" y="60"/>
                            <Anchors>
                                <Anchor point="CENTER" relativePoint="CENTER" y="10"/>
                            </Anchors>
                        </Texture>
                    </Layer>
                </Layers>
                <Scripts>
                    <OnClick>
                        eSaithBagFilter_ShowFilterRarity(self, button)
                        eSaithBagFilterSideTabs_OnClick(self, button)
                    </OnClick>
                    <OnShow>
                        CharacterFrame_TabBoundsCheck(self);
                    </OnShow>
                    <OnEnter function="eSaithBagFilterTab_OnEnter" />
                    <OnLeave function="OnGameToolTipLeave" />
                </Scripts>
            </Button>
            <Button name="$parentTab4" inherits="ItemButtonTemplate" id="4">
                <Anchors>
                    <Anchor point="TOP" relativePoint="BOTTOM" relativeTo="$parentTab3" y="0"/>
                </Anchors>
                <Layers>
                    <Layer level="OVERLAY" setAllPoints="true">
                        <Texture name="$parentTexture" file="Interface\ICONS\Ability_BossGorefiend_GorefiendsCorruption" >
                            <Size x="35" y="35"/>
                            <Anchors>
                                <Anchor point="CENTER" relativePoint="CENTER"/>
                            </Anchors>
                        </Texture>
                    </Layer>
                </Layers>
                <Scripts>
                    <OnClick>
                        eSaithBagFilterSideTabs_OnClick(self, button)
                        eSaithBagFilter_ShowCharacterInfo(self, button)
                    </OnClick>
                    <OnShow>
                        CharacterFrame_TabBoundsCheck(self);
                    </OnShow>
                    <OnEnter function="eSaithBagFilterTab_OnEnter" />
                    <OnLeave function="OnGameToolTipLeave" />
                </Scripts>
            </Button>            
            <Button name="$parentTab5" inherits="ItemButtonTemplate" id="5">
                <Anchors>
                    <Anchor point="TOP" relativePoint="BOTTOM" relativeTo="$parentTab4" y="0"/>
                </Anchors>
                <Layers>
                    <Layer level="OVERLAY" setAllPoints="true">
                        <Texture name="$parentTexture" file="Interface\ICONS\Ability_RewindTime" >
                            <Size x="35" y="35"/>
                            <Anchors>
                                <Anchor point="CENTER" relativeTo="$parent" relativePoint="CENTER" />
                            </Anchors>
                        </Texture>
                    </Layer>
                </Layers>
                <Scripts>
                    <OnClick>
                        eSaithBagFilter_ShowOptions(self, button)
                        eSaithBagFilterSideTabs_OnClick(self, button)
                    </OnClick>
                    <OnEnter function="eSaithBagFilterTab_OnEnter" />
                    <OnLeave function="OnGameToolTipLeave" />
                </Scripts>
            </Button>
        </Frames>
        <Scripts>
            <OnLoad function="eSaithBagFilterSideTabs_OnLoad" />
            <OnShow function="eSaithBagFilterSideTabs_OnShow" />
            <OnEnter function="eSaithBagFilterTab_OnEnter" />
            <OnLeave function="OnGameToolTipLeave" />
        </Scripts>
    </Frame>

